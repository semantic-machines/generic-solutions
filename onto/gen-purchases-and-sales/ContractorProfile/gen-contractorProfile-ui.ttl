gen:TS_ContractorProfile
  rdf:type v-ui:TemplateSpecification ;
  v-ui:forClass v-s:ContractorProfile ;
  v-ui:defaultTemplate gen:ContractorProfileTemplate ;
  v-s:loadPriority 10 ;
