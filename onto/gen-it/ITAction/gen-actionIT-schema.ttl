<http://semantic-machines.com/veda/gen-actionIT-schema>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология ОБЩИЕ РЕШЕНИЯ. Мероприятие ИТ. Схема"@ru ;
  rdfs:label "GEN ontology. Action IT. Schema"@en ;
  v-s:loadPriority 20 ;
.
# ------------------------------------------------------------ КЛАССЫ --
gen:ActionIT
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Action, v-s:Labeled ;
  v-s:labelPattern "{@.v-s:registrationNumber} {@.v-s:theme} - {@.gen:hasIT_InfrastructureObjects.rdfs:label}" ;
  v-ui:hasTemplate gen:ActionITTemplate ;
  rdfs:label "Мероприятие ИТ"@ru ;
  rdfs:label "Action IT"@en ;
.

#-----Статус
#v-s:hasStatus
gen:PS_hasStatus_ActionIT
   rdf:type v-ui:ObjectPropertySpecification ;
   v-ui:forClass gen:ActionIT;
   v-ui:forProperty v-s:hasStatus;
   v-ui:queryPrefix "'@'=='v-s:StatusPreparation' ||  '@'=='v-s:StatusMoreInformationPending' || '@'=='v-s:StatusCanceled' || '@'=='v-s:StatusExecuted' || '@'=='v-s:StatusExecution' || '@'=='v-s:StatusRejected' || '@'=='v-s:StatusRejected' || '@'=='v-s:StatusPostponed' || '@'=='v-s:StatusArchived' || '@'=='v-s:StatusDraft' || '@'=='v-s:StatusAgreement' || '@'=='v-s:StatusApproved'"^^xsd:string;
   rdfs:label "Спецификация свойства v-s:hasStatus для класса gen:ActionIT"@ru ;
   rdfs:label "v-s:hasStatus specification gen:ActionIT"@en ;
.

