<http://semantic-machines.com/veda/gen-meeting-net>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология ОБЩИЕ РЕШЕНИЯ. Встреча. Сеть"@ru ;
  rdfs:label "Gen ontology. Meeting. Net"@en ;
  v-s:loadPriority 10 ;
