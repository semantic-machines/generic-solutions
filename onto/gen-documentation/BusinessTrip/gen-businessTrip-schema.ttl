#-----------------------------------------------------------Командировка
gen:BusinessTrip
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:BusinessTrip ;
  rdfs:subClassOf v-s:Labeled ;
  rdfs:subClassOf v-s:AutomaticallyVersioned ;
  v-s:labelPattern "{@.v-s:hasBusinessTripRegistrationRecord.v-s:registrationNumber} ({@.gen:hasBusinessTripDirection.v-s:shortLabel}) {@.v-s:businessTripEmployee.rdfs:label} ({@.v-s:hasClassifierCountry.rdfs:label}, {@.v-s:dateFrom} - {@.v-s:dateTo})" ;
  rdfs:label "Командировка"@ru ;
  rdfs:label "BusinessTrip"@en ;
  v-ui:hasTemplate gen:BusinessTripTemplate ;
.

gen:TS_BusinessTrip
  rdf:type v-ui:TemplateSpecification ;
  v-ui:forClass gen:BusinessTrip ;
  v-ui:defaultTemplate gen:BusinessTripTemplate ;
  v-s:loadPriority 10 ;
.

#-----Регистрационная запись
