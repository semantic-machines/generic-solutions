@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix gen: <http://semantic-machines.com/veda/gen-schema/> .


<http://semantic-machines.com/veda/gen-organization-schema>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология ОБЩИЕ РЕШЕНИЯ. Организация. Схема."@ru ;
  rdfs:label "GEN ontology. Organization. Schema."@en ;
#  owl:versionInfo "1.3" ;
  v-s:loadPriority 6 ;
.

# ------------------------------------------------------------ КЛАССЫ --

v-s:Organization
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:OrganizationUnit ;
  rdfs:subClassOf v-s:ClassifiedRus ;
  rdfs:subClassOf v-s:Contractual ;
  rdfs:subClassOf v-s:Journaling ;
  rdfs:label "Организация"@ru ;
  rdfs:label "Organization"@en ;
  v-ui:hasModel v-s:OrganizationUnitModel ;
  v-ui:hasTemplate gen:OrganizationTemplate ;
.

# ------------------------------------------------------------
v-s:Subsidiary
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Organization ;
  rdfs:subClassOf v-s:Department ;
  rdfs:subClassOf v-s:BackwardLink ;
  rdfs:label "Филиал"@ru ;
  rdfs:label "Subsidiary"@en ;
  v-ui:hasModel v-s:OrganizationUnitModel ;
  v-ui:hasTemplate gen:SubsidiaryTemplate ;
.