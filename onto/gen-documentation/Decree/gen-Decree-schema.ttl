  v-ui:minCardinality 1 ;
  v-ui:maxCardinality 1 ;
  rdfs:label "Спецификация свойства v-s:title для класса gen:Decree"@ru ;
  rdfs:label "v-s:title property specification for gen:Decree class"@en ;
.

