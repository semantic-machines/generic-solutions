<http://semantic-machines.com/veda/gen-localRegulatoryDocument-schema>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология ОБЩИЕ РЕШЕНИЯ. Локальный нормативный документ. Схема"@ru ;
  rdfs:label "GEN ontology. Local regulatory document. Schema"@en ;
  v-s:loadPriority 20 ;
.
# ------------------------------------------------------------ КЛАССЫ --
gen:LocalRegulatoryDocument
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:LocalRegulatoryDocument ;
  rdfs:subClassOf v-s:BackwardLink ;
  rdfs:subClassOf v-s:Enumerated ;
  rdfs:subClassOf v-s:Labeled ;
  rdfs:label "Локальный нормативный документ"@ru ;
  rdfs:label "Local regulatory document"@en ;
  v-s:hasNumeration v-s:RegistrationNumberYearNumeration2_6 ;
  v-s:labelPattern "{@.v-s:registrationNumber} - {@.v-s:title}" ;
  v-ui:hasTemplate gen:LocalRegulatoryDocumentTemplate ;
  v-s:hasLinksTree gen:LocalRegulatoryDocumentLinksTree ;
.
gen:LocalRegulatoryDocumentLinksTree
  rdf:type v-s:LinksTree ;
  v-s:outProperty v-s:hasVersionOfLocalRegulatoryDocument;
  v-s:allowedType gen:VersionOfLocalRegulatoryDocument ;
 .
#v-s:hasDocumentKind
#v-s:valid
#v-s:shortLabel
#v-s:registrationNumber
#v-s:title
#rdfs:comment
#v-s:isFixedTerm
#v-s:hasComment
gen:isAccessLimited
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Ограниченный доступ"@ru ;
  rdfs:label "Is access limited"@en ;
  rdfs:range xsd:boolean ;
  rdfs:domain gen:LocalRegulatoryDocument ;
.
gen:appliesTo
  rdf:type owl:ObjectProperty ;
  rdfs:label "Распространяется на"@ru ;
  rdfs:label "Applies to"@en ;
  rdfs:range v-s:Department ;
  rdfs:range v-s:Appointment ;
  rdfs:domain gen:LocalRegulatoryDocument ;
.
gen:hasPositionDictionary
  rdf:type owl:ObjectProperty ;
  rdfs:label "Должность"@ru ;
  rdfs:label "Position"@en ;
  rdfs:range gen:PositionDictionary ;
  rdfs:domain gen:LocalRegulatoryDocument ;
.
#-----------------------------------------------------
gen:VersionOfLocalRegulatoryDocument
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:BackwardLink ;
  rdfs:subClassOf v-s:VersionOfLocalRegulatoryDocument ;
  rdfs:subClassOf v-s:Labeled ;
  rdfs:label "Версия документов"@ru ;
  rdfs:label "Version of documents"@en ;
  v-s:labelPattern "{@.v-s:registrationNumber} - {@.v-s:title}" ;
  v-ui:hasTemplate gen:VersionOfLocalRegulatoryDocumentTemplate ;
  v-s:hasLinksTree gen:VersionOfLocalRegulatoryDocumentLinksTree ;
.
gen:VersionOfLocalRegulatoryDocumentLinksTree
  rdf:type v-s:LinksTree ;
  v-s:inProperty v-s:hasVersionOfLocalRegulatoryDocument ;
  v-s:outProperty v-s:hasConfidentialDocument;
  v-s:allowedType gen:LocalRegulatoryDocument ;
.
#-----------------------------------------------------
gen:DescriptionOfChange
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:DescriptionOfChange ;
  rdfs:label "Описание изменений"@ru ;
  rdfs:label "Description of changes"@en ;
.
#-----------------------------------------------------
gen:PositionDictionary
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:UserThing ;
  rdfs:label "Должность"@ru ;
  rdfs:label "Position"@en ;
.
