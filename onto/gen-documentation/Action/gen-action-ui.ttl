<http://semantic-machines.com/veda/gen-action-ui>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология ОБЩИЕ РЕШЕНИЯ. Мероприятие. UI"@ru ;
  rdfs:label "GEN ontology. Action. UI"@en ;
  v-s:loadPriority 20 ;
.

# ------------------------------------------------------------ СТРОКИ --
gen:FullNameOfEmployeeBundle
  rdf:type v-s:Bundle ;
  rdfs:label "ФИО сотрудника"@ru ;
  rdfs:label "Full name of the employee"@en ;
.

# ------------------------------------------------------------ ШАБЛОНЫ --
gen:CS_Action
  rdf:type v-s:ClassSpecification ;
  rdfs:label "Спецификация класса v-s:Action"@ru, "Class specification v-s:Action"@en ;
  #v-s:hasApplication gen:TechnicalDocumentApplication;
  #v-s:hasAspect gen:DevelopmentAndReconstructionAspect ;
  v-ui:forClass v-s:Action;
  v-s:shelfLife 9999;
  v-s:listProperties v-s:attachment, v-s:hasComment, v-s:hasAction;
  v-s:hasDeletedProperties v-s:attachment, v-s:hasComment, v-s:hasAction;
  v-ui:defaultTemplate gen:ActionTemplate;
  v-s:labelPattern "{@.v-s:registrationNumber} - {@.v-s:description [0,100]}" ;
  v-s:loadPriority 10 ;
.
gen:ActionTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон Мероприятия"@ru ;
  rdfs:label "Action template"@en ;
  v-ui:forClass v-s:Action;
  v-ui:template "gen_ActionTemplate.js" ;
.

