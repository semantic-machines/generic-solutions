<http://semantic-machines.com/veda/gen-technicalDocument-schema>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология ОБЩИЕ РЕШЕНИЯ. Технический документ. Схема"@ru ;
  rdfs:label "GEN ontology. Technical document. Schema"@en ;
  v-s:loadPriority 20 ;
.
# ------------------------------------------------------------ КЛАССЫ --

#------------------------------------------------------ Технический документ --
gen:TechnicalDocument
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:UserThing ;
  rdfs:subClassOf v-s:Journaling ;
  rdfs:subClassOf v-s:Projectable ;
  rdfs:subClassOf v-s:Temporal ;
  rdfs:subClassOf v-s:Maintained ;
  rdfs:subClassOf v-s:Registered ;
  rdfs:subClassOf v-s:Labeled ;
  rdfs:subClassOf v-s:Deliverable2 ;
  rdfs:subClassOf v-s:Enumerated ;
  rdfs:subClassOf v-s:Financial ;
  rdfs:subClassOf v-s:BackwardLink ;
  rdfs:subClassOf v-s:ClassifiedRus ;
  v-s:labelPattern "{@.v-s:shortLabel} - {@.v-s:title} - {@.v-s:hasClassifierSectionOfProjectDocumentation.v-s:shortLabel} - {@.v-s:hasClassifierMarkOfWorkingDrawingsSet.v-s:shortLabel}" ;
  rdfs:label "Технический документ"@ru ;
  rdfs:label "Technical document"@en ;
  v-ui:hasTemplate gen:TechnicalDocumentTemplate ;
  v-s:hasNumeration v-s:RegistrationNumberYearNumeration2_6 ;
.

#-----Регистрационный номер
#v-s:registrationNumber

#-----Номер.Шифр документа разработчика
#v-s:shortLabel

#-----Дата документа
#v-s:registrationDate

#-----Название
#v-s:title
gen:PS_title_TechnicalDocument 
	rdf:type v-ui:PropertySpecification ;
	v-ui:forClass gen:TechnicalDocument ;
	v-ui:forProperty v-s:title ;
	v-ui:minCardinality 1 ;
	v-ui:maxCardinality 1 ;
	rdfs:label "Спецификация свойства v-s:title для класса gen:TechnicalDocument"@ru ;
	rdfs:label "v-s:title property specification for gen:TechnicalDocument class"@en ;
.

#-----Стадия разработки документа
#v-s:hasLifecycleStage
gen:PS_hasLifecycleStage_TechnicalDocument
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass gen:TechnicalDocument ;
	v-ui:forProperty v-s:hasLifecycleStage ;
	v-ui:queryPrefix "('rdf:type'=='v-s:LifecycleStage' && ('v-s:hasParentLink'=='d:a6uu8yexul97649zuczu37abf4f' || 'v-s:hasParentLink'=='d:i5kydruq51ybqqw5wbvgqevtqb'|| 'v-s:hasParentLink'=='d:lq47gp7z6dg6e7xeuv2p6l1rvj')) || '@'=='d:lq47gp7z6dg6e7xeuv2p6l1rvj'"^^xsd:string ;
	v-ui:minCardinality 1 ;
	v-ui:maxCardinality 1 ;
	rdfs:label "Спецификация свойства v-s:hasLifecycleStage для класса gen:TechnicalDocument"@ru ;
	rdfs:label "v-s:hasLifecycleStage property specification for gen:TechnicalDocument class"@en ;
.

#-----Раздел проектной документации
#v-s:hasClassifierSectionOfProjectDocumentation
gen:PS_hasClassifierSectionOfProjectDocumentation_TechnicalDocument
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass gen:TechnicalDocument ;
	v-ui:forProperty v-s:hasClassifierSectionOfProjectDocumentation ;
	v-ui:minCardinality 0 ;
	v-ui:maxCardinality 100 ;
	rdfs:label "Спецификация свойства v-s:hasClassifierSectionOfProjectDocumentation для класса gen:TechnicalDocument"@ru ;
	rdfs:label "v-s:hasClassifierSectionOfProjectDocumentation property specification for gen:TechnicalDocument class"@en ;
.
#-----Вид документа
#v-s:hasDocumentKind
gen:PS_hasDocumentKind_TechnicalDocument
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass gen:TechnicalDocument ;
	v-ui:forProperty v-s:hasDocumentKind ;
	#Техническая документация
	v-ui:queryPrefix "('rdf:type'=='v-s:DocumentKind' && 'v-s:hasParentLink'=='d:zh95m47jqjtl4dtq68h4zsm2')"^^xsd:string ;
	v-ui:minCardinality 1 ;
	v-ui:maxCardinality 1 ;
	rdfs:label "Спецификация свойства v-s:hasDocumentKind для класса gen:TechnicalDocument"@ru ;
	rdfs:label "v-s:hasDocumentKind property specification for gen:TechnicalDocument class"@en ;
.

#-----Марка комплекта рабочих чертежей
#v-s:hasClassifierMarkOfWorkingDrawingsSet
gen:PS_hasClassifierMarkOfWorkingDrawingsSet_TechnicalDocument 
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass gen:TechnicalDocument ;
	v-ui:forProperty v-s:hasClassifierMarkOfWorkingDrawingsSet ;
	v-ui:minCardinality 0 ;
	v-ui:maxCardinality 100 ;
	rdfs:label "Спецификация свойства v-s:hasClassifierMarkOfWorkingDrawingsSet для класса gen:TechnicalDocument"@ru ;
	rdfs:label "v-s:hasClassifierMarkOfWorkingDrawingsSet property specification for gen:TechnicalDocument class"@en ;
.

#-----Конструкторская заявка

#-----Документы, входящие в комплект
gen:hasTechnicalDocument
  rdf:type owl:ObjectProperty ;
  rdfs:label "Документы, входящие в комплект"@ru ;
  rdfs:label "Documents included in the kit"@en ;
  rdfs:range gen:TechnicalDocument ;
  rdfs:domain gen:TechnicalDocument ;
.
gen:PS_hasTechnicalDocument_TechnicalDocument
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass gen:TechnicalDocument ;
	v-ui:forProperty gen:hasTechnicalDocument ;
	v-ui:minCardinality 0 ;
	v-ui:maxCardinality 1000 ;
	rdfs:label "Спецификация свойства gen:hasTechnicalDocument для класса gen:TechnicalDocument"@ru ;
	rdfs:label "gen:hasTechnicalDocument property specification for gen:TechnicalDocument class"@en ;
.

#-----Родительский комплект документов 
#v-s:backwardTarget 
gen:PS_backwardTarget_TechnicalDocument
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass gen:TechnicalDocument ;
	v-ui:forProperty v-s:backwardTarget ;
	v-ui:minCardinality 0 ;
	v-ui:maxCardinality 1 ;
	v-ui:queryPrefix "'rdf:type'=='gen:TechnicalDocument'" ;
	rdfs:label "Спецификация v-s:backwardTarget для класса gen:TechnicalDocument"@ru ;
